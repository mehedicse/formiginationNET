
<script src="~/js/ops/profile/Medical.js" asp-file-version="true"></script>
<div class="row">

    <div class="form-horizontal">
        
        <hr />
        <div asp-validation-summary="ValidationSummary.All" class="text-danger"></div>
       
                <input name="MedicalId" type="hidden" id="txtMedicalId" class="k-textbox" />

            
        <div class="form-group">
            <label for="DoctorName" class="col-md-4 control-label">Doctor Name</label>
            <div class="col-md-6">
                <input name="DoctorName" id="txtDoctorName" class="k-textbox" />

            </div>
        </div>
        <div class="form-group">
            <label for="hospitalOrClinic" class="col-md-4 control-label">Hospital Or Clinic</label>
            <div class="col-md-6">
                <input name="hospitalOrClinic" id="txthospitalOrClinic" class="k-textbox" />

            </div>
        </div>
        <div class="form-group">
            <label for="prescriptionOrReport" class="col-md-4 control-label">Prescription Or Report</label>
            <div class="col-md-6">
                <input name="prescriptionOrReport" id="txtprescriptionOrReport" class="k-textbox" />

            </div>
        </div>
        <div class="form-group">
            <label for="TreatementFrom" class="col-md-4 control-label">Treatement From</label>
            <div class="col-md-6">
                <input name="TreatementFrom" id="txtTreatementFrom" class="k-textbox" />

            </div>
        </div>
        <div class="form-group">
            <label for="TreatementTo" class="col-md-4 control-label">Treatement To</label>
            <div class="col-md-6">
                <input name="TreatementTo" id="txtTreatementTo" class="k-textbox" />

            </div>
        </div>
        <div class="form-group">
            <label for="TreatementPurpose" class="col-md-4 control-label">Treatement Purpose</label>
            <div class="col-md-6">
                <input name="TreatementPurpose" id="txtTreatementPurpose" class="k-textbox" />

            </div>
        </div>
        <div class="form-group">
            <label for="Description" class="col-md-4 control-label">Description</label>
            <div class="col-md-6">
                <input name="Description" id="txtDescription" class="k-textbox" />

            </div>
        </div>
        

        <div class="form-group">
            <div class="col-md-offset-4 col-md-6">
                <button type="submit" id="btnMedicalSave" class="btn btn-default">Save</button>
            </div>
        </div>
    </div>
</div>